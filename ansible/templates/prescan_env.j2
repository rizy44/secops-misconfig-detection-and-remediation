# Pre-Scan Node Environment Variables
# Generated by Ansible

# Node identification
PRESCAN_NODE_NAME=prescan-node
PRESCAN_CONFIG=/app/config/prescan_config.yml

# Network configuration
ADMIN_CIDR={{ admin_cidr }}
INTERNAL_CIDR={{ internal_cidr }}

# SecOps App integration
SECOPS_API_URL=http://{{ secops_app_ip }}:8000

# Logging
LOG_LEVEL={{ log_level | default('INFO') }}

# Webhook notifications (optional)
WEBHOOK_URL={{ webhook_url | default('') }}

# Python settings
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
